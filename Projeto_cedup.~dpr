program Projeto_cedup;

uses
  Forms,
  U_DM in 'U_DM.pas' {DataModule1: TDataModule},
  U_PRINCIPAL in 'U_PRINCIPAL.pas' {F_PRINCIPAL},
  U_BOLETINS in 'U_BOLETINS.pas' {F_BOLETINS},
  U_TURMAS in 'U_TURMAS.pas' {F_TURMAS},
  U_MATERIAS in 'U_MATERIAS.pas' {F_MATERIA},
  U_GRADE in 'U_GRADE.pas' {F_GRADE},
  U_CURSO in 'U_CURSO.pas' {F_CURSO},
  U_RELATORIO_TURMAS in 'U_RELATORIO_TURMAS.pas' {F_RELATORIO_TURMAS},
  U_RELATORIO_GRADE in 'U_RELATORIO_GRADE.pas' {F_RELATORIO_GRADE},
  U_CADASTRO_PESSOA in 'U_CADASTRO_PESSOA.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TDM, DM);
  Application.CreateForm(TF_PRINCIPAL, F_PRINCIPAL);
  Application.CreateForm(TF_BOLETINS, F_BOLETINS);
  Application.CreateForm(TF_TURMAS, F_TURMAS);
  Application.CreateForm(TF_MATERIA, F_MATERIA);
  Application.CreateForm(TF_GRADE, F_GRADE);
  Application.CreateForm(TF_CURSO, F_CURSO);
  Application.CreateForm(TF_RELATORIO_TURMAS, F_RELATORIO_TURMAS);
  Application.CreateForm(TF_RELATORIO_GRADE, F_RELATORIO_GRADE);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
